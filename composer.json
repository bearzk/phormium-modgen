{
    "name": "phormium/modgen",
    "type": "library",
    "description": "Model class generator for Phormium.",
    "keywords": ["database","orm","php","mapping", "generator"],
    "homepage": "https://github.com/ihabunek/phormium-modgen",
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Habunek",
            "email": "ivan.habunek@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "phormium/phormium": "~0.5",
        "symfony/console": "~2.4"
    },
    "require-dev": {
        "symfony/finder": "~2.4"
    },
    "autoload": {
        "psr-0": {"Phormium\\ModGen": "src/"}
    },
    "support": {
        "issues": "https://github.com/ihabunek/phormium-modgen/issues"
    },
    "bin": ["bin/modgen"]
